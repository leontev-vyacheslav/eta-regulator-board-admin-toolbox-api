{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
    },
    "pylint.args": [
        "--disable=missing-module-docstring,missing-function-docstring,missing-class-docstring"
    ]
}